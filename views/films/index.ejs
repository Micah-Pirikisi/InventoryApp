<!DOCTYPE html>
<html>
  <head>
    <title>Films</title>
  </head>
  <body>
    <h1>All Films</h1>
    <a href="/films/new">+ Add New Film</a>
    <ul>
      <% films.forEach(film => { %>
      <li>
        <a href="/films/<%= film.film_id %>"><%= film.title %></a>
      </li>
      <% }) %>
    </ul>
  </body>
</html>
